{{> navbarPartial this}}

<div class="wrapper">

  {{#each entries}}
  <div class="entrybox">

    <h1><a href="/entries/{{this._id}}">{{this.title}}</a></h1>
    <div class="entry-date">{{this.time}}</div>
    <div class="entry-body">{{this.entryBody}}</div>

    {{#if this.tags}}
    <div class="tags">{{this.tags}}</div>
    {{/if}}


  </div>
  {{/each}}

</div>