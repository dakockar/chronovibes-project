
{{> navbarPartial this}}

<div class="wrapper">

<button class="btns" onclick="(createCal(cal, 2021, 3))">Open calendar</button>
 <div class="cal"></div> 


{{#if entries}}
  {{#each entries}}
  <div class="entrybox">

    <h1><a href="/entries/{{this._id}}">{{this.title}}</a></h1>
    <div class="entry-date">{{dateFormat this.time "MMMM Do YYYY"}}</div>
    <div class="entry-body">{{this.entryBody}}</div>

    {{#if this.tags}}
    <div class="tags">{{this.tags}}</div>
    {{/if}}


  </div>
  {{/each}}
{{else}}
  <div>No entries</div>
{{/if}}
</div>



<script>
  //TODO can this maybe be defined somewhere else? 
  let cal = document.querySelector('.cal');
</script>